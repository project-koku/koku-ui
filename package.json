{
  "name": "koku-ui",
  "version": "0.0.1",
  "description": "User Interface for Koku Application",
  "main": "index.js",
  "repository": "https://github.com/project-koku/koku-ui.git",
  "author": "Red Hat",
  "license": "GNU AGPLv3",
  "private": true,
  "scripts": {
    "build": "yarn build:prod",
    "build:prod": "fec build",
    "check:updates": "npx npm-check-updates",
    "check:milestone": "npx npm-check-updates -t newest -f '/^(@patternfly|@redhat-cloud-services)/'",
    "check:milestone:update": "npx npm-check-updates -t newest -f '/^(@patternfly|@redhat-cloud-services)/' -u",
    "clean": "rimraf dist .cache",
    "install:pkgs": "yarn clean && yarn install",
    "lint": "yarn lint:ts",
    "lint:ts": "eslint src",
    "lint:ts:fix": "eslint src --fix",
    "patch:hosts": "fec patch-etc-hosts",
    "start": "fec dev",
    "start:csc": "CLOUD_SERVICES_CONFIG_PORT=8889 npm start",
    "start:ephemeral": "EPHEMERAL_PORT=8000 npm start",
    "start:local:api": "LOCAL_API_PORT=8000 LOCAL_API_HOST=localhost KEYCLOAK_PORT=4020 npm start",
    "stats": "yarn build:prod --profile --json > stats.json",
    "test": "jest --no-cache",
    "test:clean": "jest --clearCache",
    "test:update": "yarn test:clean && jest --updateSnapshot",
    "translations": "yarn translations:extract && yarn translations:compile && yarn translations:compile:ast && yarn translations:datafile",
    "translations:test": "yarn translations:extract && yarn translations:compile && yarn translations:compile:ast && yarn translations:syncTranslations --locale fr && yarn translations:syncTranslations --locale de && yarn translations:datafile",
    "translations:clean": "rm -f `/bin/ls -1 locales/*.json | /usr/bin/egrep -v 'data|translations'.json`",
    "translations:extract": "npx formatjs extract src/locales/*.ts --out-file build/messages/src/Messages.json",
    "translations:compile": "npx formatjs compile build/messages/src/Messages.json --out-file locales/translations.json",
    "translations:compile:ast": "npx formatjs compile build/messages/src/Messages.json --out-file locales/en.json --ast",
    "translations:datafile": "node scripts/createDataJson.js",
    "translations:syncTranslations": "node scripts/syncTranslations.js",
    "verify": "yarn lint && yarn test && yarn build"
  },
  "dependencies": {
    "@patternfly/patternfly": "4.217.1",
    "@patternfly/react-charts": "6.94.7",
    "@patternfly/react-core": "4.252.0",
    "@patternfly/react-icons": "4.92.6",
    "@patternfly/react-styles": "4.91.6",
    "@patternfly/react-table": "4.111.5",
    "@patternfly/react-tokens": "4.93.6",
    "@redhat-cloud-services/frontend-components": "^3.9.21",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.12",
    "@redhat-cloud-services/frontend-components-translations": "^3.2.5",
    "@redhat-cloud-services/frontend-components-utilities": "^3.3.4",
    "@redhat-cloud-services/rbac-client": "^1.0.116",
    "@unleash/proxy-client-react": "^3.4.0",
    "axios": "^1.1.3",
    "date-fns": "^2.29.3",
    "js-file-download": "^0.4.12",
    "lodash": "^4.17.21",
    "qs": "^6.11.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^17.0.2",
    "react-intl": "^6.2.1",
    "react-redux": "^8.0.4",
    "react-router-dom": "^5.3.4",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "typesafe-actions": "^5.1.0",
    "unleash-proxy-client": "^2.3.0",
    "victory-core": "36.6.7",
    "xstate": "^4.34.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.1.3",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.2.6",
    "@redhat-cloud-services/frontend-components-config": "^4.6.28",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.1",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@xstate/test": "^0.5.1",
    "@yarnpkg/lockfile": "^1.1.0",
    "aphrodite": "^2.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.26.0",
    "eslint-plugin-formatjs": "^4.3.4",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsdoc": "^39.4.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-patternfly-react": "^4.80.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-testing-library": "^5.9.1",
    "git-revision-webpack-plugin": "^5.0.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-mock-axios": "^4.7.0-beta",
    "jws": "^4.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "user-event": "^4.0.0",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "resolutions": {
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.9",
    "eslint-plugin-rulesdir": "^0.2.1"
  },
  "insights": {
    "appname": "cost-management"
  }
}
