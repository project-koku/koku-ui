    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "git-revision-webpack-plugin": "^5.0.0",
    "glob": "^7.2.0",
    "html-loader": "^3.1.0",
    "html-replace-webpack-plugin": "^2.6.0",
    "html-webpack-plugin": "^5.5.0",
    "https-proxy-agent": "^5.0.0",
    "inquirer": "^8.2.0",
    "jest": "^27.5.1",
    "jws": "^4.0.0",
