    "@types/jest": "26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "@xstate/test": "0.4.2",
    "@yarnpkg/lockfile": "1.1.0",
    "aphrodite": "2.4.0",
