{
  "name": "@koku-ui/koku-ui-onprem",
  "version": "0.0.1",
  "description": "User Interface for Cost Management OnPrem",
  "main": "index.js",
  "repository": "https://github.com/project-koku/koku-ui.git",
  "author": "Red Hat",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=22.20.0",
    "npm": ">=11.6.2"
  },
  "scripts": {
    "build": "NODE_ENV=production npm run ts-node ../../node_modules/.bin/webpack --mode=production",
    "check:dependencies": "npx npm-check-updates",
    "check:dependencies:update": "npx npm-check-updates -u",
    "clean": "rimraf dist",
    "start": "npm run ts-node ../../node_modules/.bin/webpack serve --mode=development --color --progress",
    "start:prod": "npm run ts-node ../../node_modules/.bin/webpack serve --mode=production --color --progress",
    "lint": "npm-run-all lint:*",
    "lint:ts": "eslint src",
    "lint:ts:fix": "eslint src --fix",
    "test": "",
    "test:cypress": "cypress run --config-file ./cypress/cypress.config.ts",
    "test:cypress:open": "cypress open --config-file ./cypress/cypress.config.ts",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'"
  },
  "dependencies": {
    "@patternfly/patternfly": "6.4.0",
    "@patternfly/react-charts": "8.4.1",
    "@patternfly/react-component-groups": "6.4.0",
    "@patternfly/react-core": "6.4.1",
    "@patternfly/react-icons": "6.4.0",
    "@patternfly/react-table": "6.4.1",
    "@patternfly/react-tokens": "6.4.0",
    "@scalprum/core": "0.9.0",
    "@scalprum/react-core": "0.11.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^7.1.14",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "typesafe-actions": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "css-minimizer-webpack-plugin": "^7.0.4",
    "cypress": "^15.9.0",
    "sass": "^1.97.3",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.105.0",
    "webpack-dev-server": "^5.2.3"
  }
}
