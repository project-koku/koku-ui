{
  "name": "@koku-ui/koku-ui-hccm",
  "version": "0.0.1",
  "description": "Cost Management UI",
  "main": "index.js",
  "repository": "https://github.com/project-koku/koku-ui.git",
  "author": "Red Hat",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=22.20.0",
    "npm": ">=11.6.2"
  },
  "scripts": {
    "build": "npm run build:prod",
    "build:prod": "fec build",
    "check:dependencies": "npx npm-check-updates",
    "check:dependencies:update": "npx npm-check-updates -u",
    "clean": "rimraf dist .cache .swc",
    "codemods": "npx @patternfly/pf-codemods@latest src",
    "lint": "npm-run-all lint:*",
    "lint:ts": "eslint src",
    "lint:ts:fix": "eslint src --fix",
    "patch:hosts": "fec patch-etc-hosts",
    "postinstall": "ts-patch install && rm -rf .cache",
    "release:stage": "sh ../../scripts/release-branch.sh -s",
    "release:prod": "sh ../../scripts/release-branch.sh -p",
    "release:app-interface:stage": "sh ../../scripts/release-app-interface.sh -s",
    "release:app-interface:prod": "sh ../../scripts/release-app-interface.sh -p",
    "start": "fec dev",
    "start:csb": "CLOUD_SERVICES_BACKEND_PORT=8000 npm start",
    "start:csb:ros": "FEC_STATIC_PORT=8003 npm run start:csb",
    "start:ephemeral": "EPHEMERAL_PORT=8000 npm start",
    "start:hmr": "HMR=true npm start",
    "start:local:api": "LOCAL_API_PORT=8000 LOCAL_API_HOST=localhost KEYCLOAK_PORT=4020 npm start",
    "start:ros": "FEC_STATIC_PORT=8003 npm run start",
    "start:static": "fec static --port=8004 --config ../../node_modules/@redhat-cloud-services/frontend-components-config/bin/prod.webpack.config.js",
    "stats": "npm run build:prod --profile --json > stats.json",
    "test": "jest --no-cache --passWithNoTests",
    "test:clean": "jest --clearCache",
    "test:coverage": "jest --coverage",
    "test:update": "npm run test:clean && jest --updateSnapshot"
  },
  "scripts-info": {
    "release:stage": "Creates a PR to update the stage-hccm branch",
    "release:prod": "Creates a PR to update the prod-hccm branch",
    "release:app-interface:stage": "Creates an MR to update app-interface with latest SHA ref from stage-hccm branch",
    "release:app-interface:prod": "Creates an MR to update app-interface with latest SHA ref from prod-hccm branch",
    "start": "Run Koku UI",
    "start:csb": "Run Koku UI with local Cloud Services Backend",
    "start:csb:ros": "Run Koku UI with local Cloud Services Backend, while ROS runs statically (for running both UIs with CSB)",
    "start:ephemeral": "Run Koku UI with ephemeral env",
    "start:hmr": "Run Koku UI with hot module replacement (experimental)",
    "start:local:api": "Run Koku UI with a local API",
    "start:ros": "Run Koku UI while ROS runs statically (for running Federated Modules in Koku UI)",
    "start:static": "Run Koku UI statically (experimental)"
  },
  "dependencies": {
    "@patternfly/patternfly": "6.4.0",
    "@patternfly/react-charts": "8.4.0",
    "@patternfly/react-component-groups": "6.4.0",
    "@patternfly/react-core": "6.4.0",
    "@patternfly/react-icons": "6.4.0",
    "@patternfly/react-table": "6.4.0",
    "@patternfly/react-tokens": "6.4.0",
    "@redhat-cloud-services/frontend-components": "^7.0.13",
    "@redhat-cloud-services/frontend-components-notifications": "^6.1.14",
    "@redhat-cloud-services/frontend-components-translations": "^4.0.16",
    "@redhat-cloud-services/frontend-components-utilities": "^7.0.9",
    "@redhat-cloud-services/rbac-client": "^4.2.5",
    "@reduxjs/toolkit": "^2.11.0",
    "@unleash/proxy-client-react": "^5.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.1",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@redhat-cloud-services/frontend-components-config": "^6.7.4",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.26",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/react-redux": "^7.1.34",
    "copy-webpack-plugin": "^13.0.1",
    "git-revision-webpack-plugin": "^5.0.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.9.3"
  },
  "insights": {
    "appname": "cost-management"
  }
}
